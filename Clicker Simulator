local Tab = Window:NewTab("Auto Open Egs")
local Section = Tab:NewSection("Normal World")
Section:NewToggle("Frozen Egg", "ToggleInfo", function(state)
	if state then
		getgenv().Egg1 = true;

		while wait() do
			if getgenv().Egg1 == true then
				game:GetService("ReplicatedStorage").Bindable.Client.eggOpen.openEggEvent:Fire(false, { ["Egg"] = game:GetService("Workspace").Eggs["Frozen Arctic"], ["petsBG"] = game:GetService("Players").gamepro3211.PlayerGui.eggOpenUI.moverBG.background.petsTemplate.background, ["active"] = false, ["Order"] = 4, ["detector"] = game:GetService("Workspace").Eggs["Frozen Arctic"].Detector.Detector, ["Island"] = "Ice", ["eggName"] = "Frozen Arctic", ["frame"] = game:GetService("Players").gamepro3211.PlayerGui.eggOpenUI.moverBG.background.petsTemplate, ["cost"] = 25000000 }, false)
			end
		end
	else
		getgenv().Egg1 = false;
	end
end)
