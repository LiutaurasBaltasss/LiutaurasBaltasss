	game:GetService("ReplicatedStorage").Events.Client.verifyTwitter:InvokeServer("@GamingDan9")
	game:GetService("ReplicatedStorage").Bindable.Client.errorMessage:Fire("Successfully verified your Twitter!", true, 5)
	game:GetService("ReplicatedStorage").Bindable.Client.errorMessage:Fire("Received permanent +10% Luck boost!", true, 5)
end)
Section:NewButton("Anti Afk", "ButtonInfo", function()
	local vu = game:GetService("VirtualUser")
	game:GetService("Players").LocalPlayer.Idled:connect(function()
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	end)
end)
