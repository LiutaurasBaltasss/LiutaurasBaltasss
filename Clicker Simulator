local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("⚡Clicker Simulator!⚡ made by Lagun", "Serpent")
local Tab = Window:NewTab("Autofarm")
local Section = Tab:NewSection("AutoClicker")
Section:NewToggle("Fast auto clicker", "ToggleInfo", function(state)
	if state then
		getgenv().sds46 = true;

		while wait() do
			if getgenv().sds46 == true then
				game:GetService("ReplicatedStorage").Bindable.Client.slowAutoClickerActivate:Fire()
				game:GetService("ReplicatedStorage").Bindable.Client.slowAutoClickerActivate:Fire()
			end
		end
	else
		getgenv().sds46 = false;
	end
end)

local Section = Tab:NewSection("AutoRebirth")
Section:NewToggle("1Rebirth", "ToggleInfo", function(state)
	if state then
		getgenv().RB1 = true;

		while wait() do
			if getgenv().RB1 == true then
				game:GetService("ReplicatedStorage").Events.Client.requestRebirth:FireServer(1, false, false)
			end
		end
	else
		getgenv().RB1 = false;
	end
end)
