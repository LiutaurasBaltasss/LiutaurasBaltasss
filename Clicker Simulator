local Section = Tab:NewSection("Claim")
Section:NewButton("Get All boosts", "ButtonInfo", function()
	for i,v in pairs(game.Players.LocalPlayer.Boosts:GetDescendants()) do
		if v.Name == "isActive" then
			v.Value = true
		end
	end
end)
