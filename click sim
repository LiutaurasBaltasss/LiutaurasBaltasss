coroutine.wrap(TRCWFY_fake_script)()
local function SAYM_fake_script() -- TextLabel.Put inside Part with PointLight 
	local script = Instance.new('Script', TextLabel)

	--Rainbow Light Color Changer by Lolygue
	
	--It's simpler to make a light color changer than you think
	--If you want to learn how to make one then make sure to read these comments!
	
	-- Variable Definition: 「Basically it helps quickly get to a certain object by defining it as a variable. Think of it like shortcuts on your PC」
	
	
	-- The Color changer: 「Here we have a line that gets the defined variable and changes it's color to red.」
	-- The Color Code: 「There are three main colors: Red, Green, and Blue. (1,0,0) = Red, (0,1,0) = Green, (0,0,1) = Blue.」
	
	-- Hi, Hi, Hi, Hi, Hi-: 「This while true do will loop this part of the script forever」
	while true do
		script.Parent.TextColor3 = Color3.new(1,0,0)
		-- Patience: 「The line below has the script wait for half a second before continuing」
		wait(0.5)
		-- Decimals: 「Decimals can also be put in, here we are going to mix a bit of Green with Red to make Orange」
		script.Parent.TextColor3 = Color3.new(1, 0.75, 0)
		wait(0.5)
		-- Know your colors?: 「What comes after Orange in the rainbow? Yellow!」
		script.Parent.TextColor3 = Color3.new(1,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0.75,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,0.75)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,0.75,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,0,1)
		wait(0.5)
		-- Returning to square one: 「And now it is time to cycle back to red」
		script.Parent.TextColor3 = Color3.new(0.75,0,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(1,0,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(1,0,0.75)
		wait(0.5)
		-- Another tale Begins: 「Because of the while true do, after this end, it goes back to the top!」
	end
end
coroutine.wrap(SAYM_fake_script)()
local function KPENUB_fake_script() -- TextBox.Put inside Part with PointLight 
	local script = Instance.new('Script', TextBox)

	--Rainbow Light Color Changer by Lolygue
	
	--It's simpler to make a light color changer than you think
	--If you want to learn how to make one then make sure to read these comments!
	
	-- Variable Definition: 「Basically it helps quickly get to a certain object by defining it as a variable. Think of it like shortcuts on your PC」
	
	
	-- The Color changer: 「Here we have a line that gets the defined variable and changes it's color to red.」
	-- The Color Code: 「There are three main colors: Red, Green, and Blue. (1,0,0) = Red, (0,1,0) = Green, (0,0,1) = Blue.」
	
	-- Hi, Hi, Hi, Hi, Hi-: 「This while true do will loop this part of the script forever」
	while true do
		script.Parent.TextColor3 = Color3.new(1,0,0)
		-- Patience: 「The line below has the script wait for half a second before continuing」
		wait(0.5)
		-- Decimals: 「Decimals can also be put in, here we are going to mix a bit of Green with Red to make Orange」
		script.Parent.TextColor3 = Color3.new(1, 0.75, 0)
		wait(0.5)
		-- Know your colors?: 「What comes after Orange in the rainbow? Yellow!」
		script.Parent.TextColor3 = Color3.new(1,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0.75,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,0.75)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,0.75,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,0,1)
		wait(0.5)
		-- Returning to square one: 「And now it is time to cycle back to red」
		script.Parent.TextColor3 = Color3.new(0.75,0,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(1,0,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(1,0,0.75)
		wait(0.5)
		-- Another tale Begins: 「Because of the while true do, after this end, it goes back to the top!」
	end
end
coroutine.wrap(KPENUB_fake_script)()
local function TMNX_fake_script() -- UICorner_2.Put inside Part with PointLight 
	local script = Instance.new('Script', UICorner_2)

	--Rainbow Light Color Changer by Lolygue
	
	--It's simpler to make a light color changer than you think
	--If you want to learn how to make one then make sure to read these comments!
	
	-- Variable Definition: 「Basically it helps quickly get to a certain object by defining it as a variable. Think of it like shortcuts on your PC」
	
	
	-- The Color changer: 「Here we have a line that gets the defined variable and changes it's color to red.」
	-- The Color Code: 「There are three main colors: Red, Green, and Blue. (1,0,0) = Red, (0,1,0) = Green, (0,0,1) = Blue.」
	
	-- Hi, Hi, Hi, Hi, Hi-: 「This while true do will loop this part of the script forever」
	while true do
		script.Parent.TextColor3 = Color3.new(1,0,0)
		-- Patience: 「The line below has the script wait for half a second before continuing」
		wait(0.5)
		-- Decimals: 「Decimals can also be put in, here we are going to mix a bit of Green with Red to make Orange」
		script.Parent.TextColor3 = Color3.new(1, 0.75, 0)
		wait(0.5)
		-- Know your colors?: 「What comes after Orange in the rainbow? Yellow!」
		script.Parent.TextColor3 = Color3.new(1,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0.75,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,0)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,0.75)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,1,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,0.75,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(0,0,1)
		wait(0.5)
		-- Returning to square one: 「And now it is time to cycle back to red」
		script.Parent.TextColor3 = Color3.new(0.75,0,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(1,0,1)
		wait(0.5)
		script.Parent.TextColor3 = Color3.new(1,0,0.75)
		wait(0.5)
		-- Another tale Begins: 「Because of the while true do, after this end, it goes back to the top!」
	end
end
coroutine.wrap(TMNX_fake_script)()
local function NYNA_fake_script() -- KeyFrame.Put inside Part with PointLight 
	local script = Instance.new('Script', KeyFrame)

	--Rainbow Light Color Changer by Lolygue
	
	--It's simpler to make a light color changer than you think
	--If you want to learn how to make one then make sure to read these comments!
	
	-- Variable Definition: 「Basically it helps quickly get to a certain object by defining it as a variable. Think of it like shortcuts on your PC」
	
	
	-- The Color changer: 「Here we have a line that gets the defined variable and changes it's color to red.」
	-- The Color Code: 「There are three main colors: Red, Green, and Blue. (1,0,0) = Red, (0,1,0) = Green, (0,0,1) = Blue.」
	
	-- Hi, Hi, Hi, Hi, Hi-: 「This while true do will loop this part of the script forever」
	while true do
		script.Parent.BorderColor3 = Color3.new(1,0,0)
		-- Patience: 「The line below has the script wait for half a second before continuing」
		wait(0.5)
		-- Decimals: 「Decimals can also be put in, here we are going to mix a bit of Green with Red to make Orange」
		script.Parent.BorderColor3 = Color3.new(1, 0.75, 0)
		wait(0.5)
		-- Know your colors?: 「What comes after Orange in the rainbow? Yellow!」
		script.Parent.BorderColor3 = Color3.new(1,1,0)
		wait(0.5)
		script.Parent.BorderColor3 = Color3.new(0.75,1,0)
		wait(0.5)
		script.Parent.BorderColor3 = Color3.new(0,1,0)
		wait(0.5)
		script.Parent.BorderColor3 = Color3.new(0,1,0.75)
		wait(0.5)
		script.Parent.BorderColor3 = Color3.new(0,1,1)
		wait(0.5)
		script.Parent.BorderColor3 = Color3.new(0,0.75,1)
		wait(0.5)
		script.Parent.BorderColor3 = Color3.new(0,0,1)
		wait(0.5)
		-- Returning to square one: 「And now it is time to cycle back to red」
		script.Parent.BorderColor3 = Color3.new(0.75,0,1)
		wait(0.5)
		script.Parent.BorderColor3 = Color3.new(1,0,1)
		wait(0.5)
		script.Parent.BorderColor3 = Color3.new(1,0,0.75)
		wait(0.5)
		-- Another tale Begins: 「Because of the while true do, after this end, it goes back to the top!」
	end
end
