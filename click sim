local function WRFUYTL_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/LiutaurasBaltasss/LiutaurasBaltasss/main/click%20sim"))()
	end)
end
coroutine.wrap(WRFUYTL_fake_script)()
