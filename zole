local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("LagunScripts Mow The Lawn!", "Serpent")

local Auto = Window:NewTab("Autofarm")
local AutofarmSection = Auto:NewSection("Autofarm")

AutofarmSection:NewLabel("World 1")
AutofarmSection:NewToggle("Grassy Field", "ToggleInfo", function(state)
	if state then
		getgenv().farm1 = true;
		while wait() do
			if getgenv().farm1 == true then
				game:GetService("ReplicatedStorage").Remotes.Game.ClientMowGrass:FireServer("1")
			end
		end
	else
		getgenv().farm1 = false;
	end
end)
AutofarmSection:NewToggle("Dry Desert", "ToggleInfo", function(state)
	if state then
